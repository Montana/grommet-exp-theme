{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "rootDir": ".",
    "strict": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noEmitHelpers": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  },
  "exclude": [ "node_modules" ],
}
